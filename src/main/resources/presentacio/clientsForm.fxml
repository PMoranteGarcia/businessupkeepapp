                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <Separator opacity="0.0" orientation="VERTICAL" prefWidth="30.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
